{
  "name": "llamadoro",
  "version": "1.0.0",
  "description": "Improve your fitness while you work!",
  "main": "index.js",
  "author": "Chandu Tennety <chandu.tennety@gmail.com>",
  "license": "MIT",
  "private": false,
  "elmSvgModules": [
    {
      "src": "img/*.svg",
      "name": "Gen.Graphics",
      "dest": "src/Gen/Graphics.elm"
    }
  ],
  "dependencies": {
    "tauri": "^0.12.0"
  },
  "devDependencies": {
    "@fullhuman/postcss-purgecss": "^2.3.0",
    "autoprefixer": "^9.6.1",
    "elm": "^0.19.1-2",
    "elm-hot": "^1.0.1",
    "netlify-cli": "^2.19.0",
    "node-elm-compiler": "^5.0.4",
    "parcel-bundler": "^1.12.3",
    "parcel-plugin-elm-svg": "^1.0.6",
    "sass": "^1.26.10"
  },
  "scripts": {
    "dev": "netlify dev -p 8888",
    "dev:live": "netlify dev --live",
    "deploy": "npm run build && netlify deploy",
    "deploy:prod": "netlify deploy --prod",
    "start": "npm run build-img && parcel index.html",
    "build-img": "parcel build img/*",
    "build": "npm run build-img && parcel build index.html --no-cache",
    "test": "elm-test",
    "test:watch": "elm-test --watch",
    "cypress": "cypress run",
    "cypress:open": "cypress open"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/cedricss/elm-batteries.git"
  },
  "keywords": [
    "elm",
    "parcel",
    "tailwind",
    "purgecss",
    "cypress",
    "netlify"
  ]
}
